<section class="gallery">
  <% @artists.each do |artist| %>
     <article class="card">
        <div class="img">
          <%= image_tag artist.photo.image.thumbnail %>
        </div>
      <h2 style = "text-decoration: none; padding-left: 20px; "><%= link_to artist.name, artist %></h2>
      </article>
   <% end %>
</section>






