<%= form_for [@song.artist, @song] do |f| %>

<ul>
  <% @song.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
  <% end %>
</ul>

<div class="form-group">
  <%= f.label :name %>
<div class="form-group">
  <%= f.text_field :name %>
</div>
</div>

<div class="form-group">
  <%= f.label :link %>
<div class="form-group">
  <%= f.text_field :link %>
</div>
</div>  

  <%= f.submit "Create" %>
<% end %>
